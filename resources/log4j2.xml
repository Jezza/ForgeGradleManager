<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN" packages="me.jezza.fgpm">
    <Appenders>
        <Console name="SysOut" target="SYSTEM_OUT">
            <PatternLayout pattern="[%d{HH:mm:ss}] [%t/%level]: %msg%n" />
        </Console>
    </Appenders>
    <Loggers>
        <Root level="all">
            <AppenderRef ref="SysOut" level="INFO" />
        </Root>
    </Loggers>
</Configuration>
